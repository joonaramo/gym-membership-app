{
  "name": "gym-membership-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.22.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.0",
    "mongoose-paginate-v2": "^1.4.1",
    "mongoose-unique-validator": "^2.0.3"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "jest": "26.6.0",
    "supertest": "^6.1.6"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "server": "cross-env NODE_ENV=development nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server \" \"npm run client\"",
    "build:ui": "rm -rf build && cd client && npm run build --prod && cp -r build ../",
    "deploy": "git push heroku main",
    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && git push && npm run deploy",
    "heroku-postbuild": "npm run build:ui",
    "lint": "eslint .",
    "test": "cross-env NODE_ENV=test jest --verbose --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joonaramo/gym-membership-app.git"
